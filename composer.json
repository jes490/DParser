{
    "name": "jes490/dparser",
    "description": "Simple Dice Roller for Laravel 5.5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jes490",
            "email": "aseselkin@gmail.com"
        }
    ],
    "config": {
        "bin-dir": "vendor/bin"
    },
